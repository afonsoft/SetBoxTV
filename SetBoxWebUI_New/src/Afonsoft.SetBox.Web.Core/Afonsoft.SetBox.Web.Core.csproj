    <PackageReference Include="Abp.HangFire.AspNetCore" Version="5.12.0" />
    <PackageReference Include="Abp.RedisCache" Version="5.12.0" />
  </ItemGroup>
  
  
  <ItemGroup>
    <PackageReference Include="System.Diagnostics.PerformanceCounter" Version="4.7.0" />
